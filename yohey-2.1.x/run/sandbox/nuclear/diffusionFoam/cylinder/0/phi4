FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      phi4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 -1 0 0 0 0];

internalField   uniform 1e15;

boundaryField
{
    fixedWalls
    {
        type            fixedValue;
        value           uniform 0;
    }
}

